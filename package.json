        "glob": "^7.1.3",
        "rimraf": "^2.6.3",
        "tslint": "^5.11.0",
        "typescript": "^3.1.1",
        "@types/jest": "^23.3.9",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.4"
    },
